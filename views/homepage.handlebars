<!-- Loop over each object in the projects array -->
{{#each posts as |post| }}
<div class="row mb-4">
  <div class="col-md-5">
    <h2>
        {{post.title}}
      <a href="/post/{{post.id}}">{{post.title}}</a>
    </h2>
    <p>Created by {{post.username}} on {{format_date post.date_created}}</p>
  </div>

</div>
{{/each}}